          },
          "sameAs": {
            "description": "URL of third-party webpages describing the same taxon",
            "oneOf": [
              {
                "type": "string",
                "format": "uri"
              },
              {
                "type": "array",
                "items": {
                  "type": "string",
                  "format": "uri"
                }
              }
            ]
          },
          "url": {
            "description": "URL of the item.",
            "type": "string",
            "format": "uri"
          }
        },
        "required": [
          "taxonRank",
          "name"
        ],
        "definitions":{
            "taxon": {
                "@type": "bioschemas:Taxon",
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "taxonRank": {
                    "anyOf": [
