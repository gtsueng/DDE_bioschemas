                                    }
                                  },
                                  {
                                    "type": "string",
                                    "format": "uri"
                                  },
                                  {
                                    "type": "array",
                                    "items": {
                                      "type": "string",
                                      "format": "uri"
                                    }
                                  }
                                ]
                  }
                },
                "required": [
                  "name",
                  "taxonRank"
                ]   
            },
            "propertyValue": {
                "@type": "PropertyValue",
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "value": {
                    "type": "string"    
                  }
                },
                "required": [
                ]  
            }
        }
