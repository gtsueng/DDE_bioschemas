              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "name": {
            "description": "Currently valid (zoological) or accepted (botanical) name for that taxon, with authorship and date information if known.",
